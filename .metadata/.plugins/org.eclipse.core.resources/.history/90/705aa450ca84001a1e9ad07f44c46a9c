package entity_class;

public class Transactions extends Wallet{

	private String T_id;
	private int T_ammount;
	private String Caption;
	public Transactions() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Transactions(String name, int contact_no, String email, int adhar) {
		super(name, contact_no, email, adhar);
		// TODO Auto-generated constructor stub
	}
	public Transactions(String uPI, int wallet_amt, String passWallet) {
		super(uPI, wallet_amt, passWallet);
		// TODO Auto-generated constructor stub
	}
	public Transactions(String uPI, int wallet_amt, String passWallet, String t_id, int t_ammount, String caption) {
		super(uPI, wallet_amt, passWallet);
		T_id = t_id;
		T_ammount = t_ammount;
		Caption = caption;
	}
	
	
	public Transactions(String name, int contact_no, String email, int adhar, String uPI, int wallet_amt,
			String passWallet, String t_id, int t_ammount, String caption) {
		super(name, contact_no, email, adhar, uPI, wallet_amt, passWallet);
		T_id = t_id;
		T_ammount = t_ammount;
		Caption = caption;
	}
	@Override
	public String toString() {
		return "Transactions [T_id=" + T_id + ", T_ammount=" + T_ammount + ", Caption=" + Caption + "]";
	}
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = super.hashCode();
		result = prime * result + ((Caption == null) ? 0 : Caption.hashCode());
		result = prime * result + T_ammount;
		result = prime * result + ((T_id == null) ? 0 : T_id.hashCode());
		return result;
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (!super.equals(obj))
			return false;
		if (getClass() != obj.getClass())
			return false;
		Transactions other = (Transactions) obj;
		if (Caption == null) {
			if (other.Caption != null)
				return false;
		} else if (!Caption.equals(other.Caption))
			return false;
		if (T_ammount != other.T_ammount)
			return false;
		if (T_id == null) {
			if (other.T_id != null)
				return false;
		} else if (!T_id.equals(other.T_id))
			return false;
		return true;
	}
	public String getT_id() {
		return T_id;
	}
	public void setT_id(String t_id) {
		T_id = t_id;
	}
	public int getT_ammount() {
		return T_ammount;
	}
	public void setT_ammount(int t_ammount) {
		T_ammount = t_ammount;
	}
	public String getCaption() {
		return Caption;
	}
	public void setCaption(String caption) {
		Caption = caption;
	}
	
	
	
	
	
	
}
